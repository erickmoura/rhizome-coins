logging:
 level: ERROR

database: 
  driverClass: ${MYSQL_DATABASE} 
  user: ${MYSQL_USER} 
  password: ${MYSQL_PASSWORD} 
  url: ${MYSQL_URL}
      
elastic:
  host: ${ELASTIC_HOST} 
  port: ${ELASTIC_PORT}
  scheme: ${ELASTIC_SCHEME} 
  indices:
    tickers: elastic/tickers_index_template.json
    cmc: elastic/cmc_index_template.json
    market-depth: elastic/market_depth_index_template.json
    log: elastic/log_index_template.json
